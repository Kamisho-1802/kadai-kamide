<h2>ユーザー一覧</h2>

<%= search_form_for @q, url: users_path, method: :get do |f| %>
  <%= f.label :name_cont, "名前で検索：" %>
  <%= f.search_field :name_cont %>
  <%= f.submit "検索" %>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.name, user_path(user) %></li>
  <% end %>
</ul>
<%= paginate @users %>